{
  "name": "grove",
  "version": "1.0.0",
  "description": "Lightweight library for tree data structures",
  "main": "dist/grove.js",
  "module": "dist/grove.esm.js",
  "types": "dist/grove.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mladoniczky/grove.git"
  },
  "keywords": [
    "tree",
    "trie",
    "tree-structure",
    "search-tree",
    "binary-tree",
    "data-structure"
  ],
  "author": "Milan Mladoniczky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mladoniczky/grove/issues"
  },
  "homepage": "https://github.com/mladoniczky/grove#readme",
  "files": [
    "dist",
    "LICENSE",
    "CHANGELOG.md",
    "README.md",
    "package.json",
    "!**/*.spec.*",
    "!**/test"
  ],
  "scripts": {
    "test": "npm run test:spell && npm run test:lint && npm run test:unit",
    "test:spell": "cspell -s \"{*.md,src/**/*.ts}\"",
    "test:lint": "eslint src --ext .ts",
    "test:unit": "jest",
    "doc": "typedoc  --entryPoints src/index.ts --exclude **/*.spec.* --out docs --theme default",
    "build": "tsc -p tsconfig.dts.json && rollup -c"
  },
  "dependencies": {
    "tslib": "~2.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "~8.3.0",
    "@typescript-eslint/eslint-plugin": "~5.12.0",
    "@typescript-eslint/parser": "~5.12.0",
    "cspell": "~5.18.5",
    "eslint": "~8.9.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.25.4",
    "jest": "~27.5.1",
    "jest-sonar-reporter": "~2.0.0",
    "rollup": "~2.67.3",
    "rollup-plugin-delete": "~2.0.0",
    "rollup-plugin-dts": "~4.1.0",
    "rollup-plugin-terser": "~7.0.2",
    "typedoc": "~0.22.11",
    "typescript": "~4.5.5"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter"
  }
}
